                    </xs:annotation>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="5"/>
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNuDesp">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
