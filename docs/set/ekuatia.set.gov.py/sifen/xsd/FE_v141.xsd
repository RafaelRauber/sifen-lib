            <xs:element name="dTotGralOpe" type="tMontoBase"/>               
            <xs:element name="dIVA5" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dIVA10" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dTotIVA" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dBaseGrav5" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dBaseGrav10" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dTBasGraIVA" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dLTotISC" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dTBasGravISC" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dTotalGs" minOccurs="0" type="tMontoBase"/>                
            <xs:element name="dTotCom" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dComi" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dIVAComi" minOccurs="0" type="tMontoBase"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tDE">
        <xs:annotation>
            <xs:documentation>
                Campos firmados del DE
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dDVId" type="tDVer"/>
            <xs:element name="dFecFirma" type="fecHhmmss"/>
            <xs:element name="gOpeDE" type="tgCOpeDE"/>
            <xs:element name="gTimb" type="tgDTim"/>
            <xs:element name="gDatGralOpe" type="tgDaGOC"/>
            <xs:element name="gDtipDE" type="tgDtipDE"/>
            <xs:element minOccurs="0" name="gTotSub" type="tgTotSub"/>
            <xs:element minOccurs="0" name="gCamGen" type="tgCamGen"/>
            <xs:element minOccurs="0" maxOccurs="99" name="gCamDEAsoc" type="tgCamDEAsoc"/>               
        </xs:sequence>
        <!--CDC pasa a ser atributo del rDE-->
        <xs:attribute name="Id" type="tCDC" use="required"/>
    </xs:complexType>

    <!-- Campos que identifican al documento asociado -->
    <xs:complexType name="tgCamDEAsoc">
        <xs:annotation>
            <xs:documentation>
                Campos que identifican al documento asociado
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipDocAso" type="tiTipDocAso"/>
            <xs:element name="dDesTipDocAso" type="tdDesTipDocAso"/>
            <xs:element name="dCdCDERef" type="tCDC" minOccurs="0"/>
            <xs:element name="dNTimDI" type="tdNumTim" minOccurs="0"/>
            <xs:element name="dEstDocAso" type="tdEst" minOccurs="0"/>
            <xs:element name="dPExpDocAso" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Punto de expedicion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumDocAso" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero del documento
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="7"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="iTipoDocAso" type="tiTIpoDoc" minOccurs="0"/>
            <xs:element name="dDTipoDocAso" type="tdDesTIpoDoc" minOccurs="0"/>
            <xs:element name="dFecEmiDI" type="tdFeIniS" minOccurs="0"/>
            <xs:element name="dNumComRet" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de comprobante de retencion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">                    	
                    	<xs:whiteSpace value="collapse"/>
                        <xs:maxLength value="15"/>
                        <xs:minLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumResCF" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de resolucion de Credito Fiscal
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                    	<xs:whiteSpace value="collapse"/>
                        <xs:maxLength value="15"/>
                        <xs:minLength value="15"/>                        
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Campos de uso general -->
    <xs:complexType name="tgCamGen">
        <xs:annotation>
            <xs:documentation>
                Campos complementarios comerciales de uso general
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dOrdCompra" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de orden de compra
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                    	<xs:whiteSpace value="collapse"/>
                        <xs:minLength value="1"/>
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dOrdVta" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de orden de venta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                    	<xs:whiteSpace value="collapse"/>
                        <xs:minLength value="1"/>
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dAsiento" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Número de asiento contable
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                    	<xs:whiteSpace value="collapse"/>
                        <xs:minLength value="1"/>
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgCamFuFD">
        <xs:annotation>
            <xs:documentation>
                Campos fuera de la firma digital
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dCarQR">
                <xs:annotation>
                    <xs:documentation>
                        Caracteres correspondiente al codigo QR
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="100"/>
                        <xs:maxLength value="600"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            
            <xs:element name="dInfAdic" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Información adicional de interés para el emisor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="5000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="rDE">
        <xs:sequence>

            <xs:element name="dVerFor">
                <xs:simpleType>
			<xs:restriction base="xs:integer">
			    <xs:pattern value="[1][4][1]"/>
			</xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="DE" type="tDE"/>
            <xs:element ref="ds:Signature">
                <xs:annotation>
                    <xs:documentation>
                        Firma Digital del DE
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="gCamFuFD" type="tgCamFuFD"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
