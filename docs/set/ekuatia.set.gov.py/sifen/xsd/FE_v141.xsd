                    <xs:documentation>
                        Puerto de embarque
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="5"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Campos que describen los subtotales y totales de la transaccion documentada -->
    <xs:complexType name="tgTotSub">
        <xs:annotation>
            <xs:documentation>
                F. Campos que describen los subtotales y totales de la transacción documentada
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dSubExe" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dSubExo" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dSub5" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dSub10" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dSubISC" minOccurs="0" type="tMontoBase"/>
            <xs:element name="dTotOpe" type="tMontoBase"/>                
            <xs:element name="dTotDesc" type="tMontoBase"/>               
            <xs:element name="dPorcDescTotal">
                <xs:annotation>
                    <xs:documentation>
                        Porcentaje de descuento sobre total de la operacion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                	<xs:restriction base="xs:decimal">
                        <xs:totalDigits value="11"/>
                        <xs:fractionDigits value="8"/>                        
                        <xs:minInclusive value="0"/>
						<xs:maxInclusive value="999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDescTotal" type="tMontoBase"/>
