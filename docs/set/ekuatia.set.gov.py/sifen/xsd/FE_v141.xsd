<?xml version="1.0" encoding="utf-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
        elementFormDefault="qualified"
        xmlns="http://ekuatia.set.gov.py/sifen/xsd"
        targetNamespace="http://ekuatia.set.gov.py/sifen/xsd">

    <xs:import namespace="http://www.w3.org/2000/09/xmldsig#"
               schemaLocation="xmldsig-core-schema.xsd"/>
    <xs:include schemaLocation="https://ekuatia.set.gov.py/sifen/xsd/Paises_v100.xsd"/>
    <xs:include schemaLocation="https://ekuatia.set.gov.py/sifen/xsd/Departamentos_v141.xsd"/>
    <xs:include schemaLocation="https://ekuatia.set.gov.py/sifen/xsd/Monedas_v100.xsd"/>
    <xs:include schemaLocation="https://ekuatia.set.gov.py/sifen/xsd/Unidades_Medida_v141.xsd"/>
    <xs:include schemaLocation="https://ekuatia.set.gov.py/sifen/xsd/FE_Types_v141.xsd"/>

    <xs:complexType name="tgCOpeDE">
        <xs:annotation>
            <xs:documentation>
                Campos de la operacion del Documento Electronico
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipEmi" type="tiTipEmi"/>
            <xs:element name="dDesTipEmi" type="tdDesTipEmi"/>
            <xs:element name="dCodSeg" type="tiCodSe"/>
            <xs:element name="dInfoEmi" minOccurs="0" type="tdInfoEmi"/>
            <xs:element name="dInfoFisc" minOccurs="0" type="tdInfoFisc"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgDTim">
        <xs:annotation>
            <xs:documentation>
                Campos de datos del timbrado
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTiDE" type="tiTiDE"/>
            <xs:element name="dDesTiDE" type="tdDesTiDE"/>
            <xs:element name="dNumTim" type="tdNumTim"/>
            <xs:element name="dEst" type="tdEst"/>
            <xs:element name="dPunExp" type="tdPunExp"/>
            <xs:element name="dNumDoc" type="tdNumDoc"/>
            <xs:element name="dFeIniT" type="tdFeIniT"/>
            <xs:element name="dFeFinT" type="tFecAAAAMMDDguion"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgEmis">
        <xs:annotation>
            <xs:documentation>
                Campos que identifican al emisor del Documento Electrónico DE
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dRucEm" type="tRuc"/>
            <xs:element name="dDVEmi" type="tDVer"/>
            <xs:element name="iTipCont" type="tiTipCont"/>
            <xs:element name="cTipReg" minOccurs="0" type="tcTipReg"/>
            <xs:element name="dNomEmi">
                <xs:annotation>
                    <xs:documentation>
                        Nombre o razon social del emisor del DE
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:whiteSpace value="preserve"/>
                        <xs:minLength value="4"/>
                        <xs:maxLength value="60"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNomFanEmi" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nombre de fantasía
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:whiteSpace value="preserve"/>
                        <xs:minLength value="4"/>
                        <xs:maxLength value="60"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDirEmi" type="tdDirec"/>
            <xs:element name="dNumCas">
                <xs:annotation>
                    <xs:documentation>
                        Numero de casa
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="6"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCompDir1" minOccurs="0" type="tdDirec"/>
            <xs:element name="dCompDir2" minOccurs="0" type="tdDirec2"/>               
            <xs:element name="cDepEmi" type="tDepartamentos"/>
            <xs:element name="dDesDepEmi" type="tDesDepartamento"/>
            <xs:element name="cDisEmi" minOccurs="0" type="tcDisEmi"/>
            <xs:element name="dDesDisEmi" minOccurs="0" type="tdDesDisEmi"/>
            <xs:element name="cCiuEmi" type="tcCiuEmi"/>
            <xs:element name="dDesCiuEmi" type="tdDesCiuEmi"/>
            <xs:element name="dTelEmi" type="tdTel"/>
            <xs:element name="dEmailE" type="tEmail"/>
            <xs:element name="dDenSuc" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Denominación comercial de la sucursal
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="30"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="gActEco" maxOccurs="9" type="tgActEco"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="tgActEco">
        <xs:annotation>
            <xs:documentation>
                Grupo de Campos de la Actividad Economica
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="cActEco" type="tcActEco"/>
            <xs:element name="dDesActEco" type="tdDesActEco"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="tgDatRec">
    	<xs:annotation>
            <xs:documentation>
                Campos que identifican al receptor del Documento Electrónico DE
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iNatRec" type="tiNatRec"/>
            <xs:element name="iTiOpe" type="tiTiOpe"/>
            <xs:element name="cPaisRec" type="paisType"/>
            <xs:element name="dDesPaisRe" type="tDesPais"/>
            <xs:element name="iTiContRec" minOccurs="0" type="tiTipCont"/>
            <xs:element name="dRucRec" minOccurs="0" type="tRuc"/>
            <xs:element name="dDVRec" minOccurs="0" type="tDVer"/>
            <xs:element name="iTipIDRec" minOccurs="0" type="tiTipDocRec"/>
            <xs:element name="dDTipIDRec" minOccurs="0" type="tdDtipDocRec"/>
            <xs:element name="dNumIDRec" minOccurs="0" type="tdNumDocId"/>
            <xs:element name="dNomRec" type="tdNombre"/>
            <xs:element name="dNomFanRec" minOccurs="0" type="tdNombre"/>
            <xs:element name="dDirRec" minOccurs="0" type="tdDirec"/>
            <xs:element name="dTelRec" minOccurs="0" type="tdTel"/>
            <xs:element name="dCelRec" minOccurs="0" type="tdCel"/>
            <xs:element name="dEmailRec" minOccurs="0" type="tEmail"/>
            <xs:element name="dCodCliente" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Codigo del Cliente
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="3"/>
                        <xs:whiteSpace value="collapse"/>
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgDaGOC">
        <xs:annotation>
            <xs:documentation>
               Campos Generales del Documento Electrónico DE
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dFeEmiDE" type="fecHhmmss"/>
            <xs:element name="gOpeCom" type="tgOpeCom" minOccurs="0"/>
            <xs:element name="gEmis" type="tgEmis"/>
            <xs:element name="gDatRec" type="tgDatRec"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="tgOpeCom">
        <xs:annotation>
            <xs:documentation>
                Campos inherentes a la operacion comercial
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipTra" minOccurs="0" type="tiTipTra"/>
            <xs:element name="dDesTipTra" minOccurs="0" type="tdDesTiTran"/>
            <xs:element name="iTImp" type="tiTImp"/>
            <xs:element name="dDesTImp" type="tdDesTImp"/>
            <xs:element name="cMoneOpe" type="cMondT"/>
            <xs:element name="dDesMoneOpe">
                <xs:annotation>
                    <xs:documentation>
                        Descripcion de la moneda de la operacion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="3"/>
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCondTiCam" minOccurs="0" type="tdCondTiCam"/>
            <xs:element name="dTiCam" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Tipo de cambio
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="9"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minInclusive value="1"/>
                        <xs:maxExclusive value="99999.9999"/>
                   </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen el pago de la operacion con tarjeta de credito/debito-->
    <xs:complexType name="tgPagTarCD">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el pago o entrega inicial de la operación con tarjeta de crédito/débito
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iDenTarj" type="tiDenTarj"/>
            <xs:element name="dDesDenTarj" type="tdDesDenTarj"/>
            <xs:element name="dRSProTar" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Razón social de la procesadora de tarjeta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="4"/>
                        <xs:maxLength value="60"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dRUCProTar" type="tRuc" minOccurs="0"/>
            <xs:element name="dDVProTar" type="tDVer" minOccurs="0"/>
            <xs:element name="iForProPa" type="tiForProPa"/>
            <xs:element name="dCodAuOpe" minOccurs="0" type="tdNunApOpe"/>
            <xs:element name="dNomTit" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nombre del titular de la tarjeta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="4"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumTarj" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de la tarjeta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <!--Campos que describen el pago o entrega inicial de la operación con cheque-->
    <xs:complexType name="tgPagCheq">
        <xs:annotation>
            <xs:documentation>
                	Campos que describen el pago o entrega inicial de la operación con cheque
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dNumCheq" type="tNumCheq"/>
            <xs:element name="dBcoEmi">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                    	<xs:whiteSpace value="collapse"/>
                        <xs:minLength value="4"/>
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen las cuotas-->
    <xs:complexType name="tgCuotas">
        <xs:annotation>
            <xs:documentation>
                Campos que describen las cuotas
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            
            <xs:element name="dMonCuota" type="tMontoBase4"/>               
            <xs:element name="dVencCuo" minOccurs="0" type="tFecAAAAMMDDguion"/>
        </xs:sequence>
    </xs:complexType>
    
    <!--Campos que describen las informaciones de compras publicas-->
    <xs:complexType name="tgCompPub">
        <xs:annotation>
            <xs:documentation>
                Campos que describen informaciones de compras publicas
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dModCont" type="tdModCont"/>
            <xs:element name="dEntCont " type="tdEntCont"/>
            <xs:element name="dAnoCont" type="tdAnoCont"/>
            <xs:element name="dSecCont" type="tdSecCont"/>
            <xs:element name="dFeCodCont" type="tFecAAAAMMDDguion"/>
        </xs:sequence>
    </xs:complexType>
        
    <!--Campos que componen la factura electronica FE-->
    <xs:complexType name="tgCamFE">
        <xs:annotation>
            <xs:documentation>
                Campos que componen la factura electronica
            </xs:documentation>
        </xs:annotation>        
        <xs:sequence>
        	<xs:element name="iIndPres" type="tiIndPres"/>
        	 <xs:element name="dDesIndPres" type="tdDesIndPres"/>
            <xs:element name="gCompPub" minOccurs="0" type="tgCompPub"/>
        </xs:sequence>
    </xs:complexType>

    <!--Campos de Precio descuentos por item-->
    <xs:complexType name="tgValorItem">
        <xs:annotation>
            <xs:documentation>
                Campos que describen los precios, descuentos y valor total por item
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dPUniProSer" type="tMontoBase"/>               
            <xs:element name="dDescItem" type="tMontoBase"/>
            <xs:element name="dPorcDesIt" minOccurs="0"> 
                <xs:annotation>
                    <xs:documentation>
                        Porcentaje de descuento por item
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                	<xs:restriction base="xs:decimal">
                        <xs:totalDigits value="11"/>
                        <xs:fractionDigits value="8"/>                        
                        <xs:minInclusive value="0"/>
						<xs:maxInclusive value="999.99999999"/>
                    </xs:restriction>                    
                </xs:simpleType>
            </xs:element>
            <xs:element name="dTotOpeItem" type="tMontoBase"/>
            <xs:element name="dTiCamIt" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Tipo de cambio por ítem
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="9"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="99999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dTotOpeGs" minOccurs="0" type="tMontoBase"/>                
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen el IVA de la operacion-->
    <xs:complexType name="tgCamIVA">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el IVA de la operación por ítem
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iAfecIVA" type="tiAfecIVA"/>
            <xs:element name="dDesAfecIVA" type="tdDesAfecIVA"/>
            <xs:element name="dPropIVA" type="tdPropIVA"/>
            <xs:element name="dTasaIVA" type="tdTasaIVA"/>
            <xs:element name="dBasGravIVA" type="tMontoBase"/>                
            <xs:element name="dLiqIVAItem" type="tMontoBase"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Campos que describen el ISC de la operacion -->
    <xs:complexType name="tgCamISC">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el ISC de la operacion por ítem
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="cCatISC" type="tcCatISC"/>
            <xs:element name="dDesCatISC" type="tdDesCatISC"/>
            <xs:element name="cTasaISC" type="tcTasaISC"/>
            <xs:element name="dBaseGravISC" type="tMontoBase"/>               
            <xs:element name="dLiqISCItem" type="tMontoBase"/>	                
        </xs:sequence>
    </xs:complexType>

    <!--Grupo de rastreo de la mercaderia-->
    <xs:complexType name="tgRasMerc">
        <xs:annotation>
            <xs:documentation>
                Grupo de rastreo de la mercaderia
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dNumLote" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                    	<xs:maxLength value="80"/>
                    	<xs:pattern value=".+"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dVencMerc" minOccurs="0" type="tFecAAAAMMDDguion"/>
            <xs:element name="dNSerie" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de serie
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Sector Automovilistico Vehiculos nuevos-->
    <xs:complexType name="tgVehNuevo">
        <xs:annotation>
            <xs:documentation>
                Detalle de vehiculos nuevos
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipOpVN" type="tiTipOpVN" minOccurs="0"/>
            <xs:element name="dDesTipOpVN" type="tdDesTipOpVN" minOccurs="0"/>
            <xs:element name="dChasis" type="tdChasis" minOccurs="0"/>
            <xs:element name="dColor" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dPotencia" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Potencia del motor (CV)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCapMot" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Capacidad del motor (cc)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dPNet" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Peso Neto (Toneladas)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="10"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dPBruto" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Peso bruto (Toneladas)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="10"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="iTipCom" type="tiTipCom" minOccurs="0"/>
            <xs:element name="dDesTipCom" type="tdDesTipCom" minOccurs="0"/>
            <xs:element name="dNroMotor" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de motor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="21"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCapTracc" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Capacidad maxima de traccion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="10"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dAnoFab" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Anho de fabricacion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:pattern value="[0-9]{4}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!--<xs:element name="iTipoPint" type="tiTipoPint"/>-->
            <!--<xs:element name="dDesTP" type="tdDesTP"/>-->
            <xs:element name="cTipVeh" minOccurs="0" type="tcTipVeh"/>
            <!--<xs:element name="iCondVeh" type="tiCondVeh"/>-->
            <!--<xs:element name="dDesCondVeh" type="tdDesCondVeh"/>-->
            <xs:element name="dCapac" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Capacidad máxima de pasajeros
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Sector Energia Electrica-->
    <xs:complexType name="tgGrupEner">
        <xs:annotation>
            <xs:documentation>
                Grupo del sector de energia electrica
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dNroMed" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de medidor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dActiv" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Codigo de actividad
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCateg" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Codigo de categoría
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
