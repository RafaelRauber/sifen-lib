        <xs:annotation>
            <xs:documentation>
                Campos que describen la operación a crédito
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>        
            <xs:element name="iCondCred">
                <xs:annotation>
                    <xs:documentation>
                        Condicion de la operacion de credito
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                    	<xs:totalDigits value="1"/>
                    	<!-- 1- Plazo -->
                        <xs:enumeration value="1"/>
                        <!-- 2- Cuota -->
            			<xs:enumeration value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDCondCred" type="tdDCondCred"/>
          
            <!--TODO Ver para mejorar la descripcion del plazo de credito o normalizarlo a una unidad
            estandar para mayor facilidad de evaluacion-->
            <xs:element name="dPlazoCre" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Plazo del crédito
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                    	<xs:whiteSpace value="collapse"/>
                        <xs:minLength value="2"/>
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            
            <xs:element name="dCuotas" minOccurs="0">
            	<xs:annotation>
                    <xs:documentation>
                        Cantidad de cuotas
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            
            <xs:element name="dMonEnt" minOccurs="0" type="tMontoBase4"/>                
            <xs:element name="gCuotas" minOccurs="0" maxOccurs="999" type="tgCuotas"/>
        </xs:sequence>
    </xs:complexType>
    
    <!--Campos que componen la Autofatura electronica-->
    <xs:complexType name="tgCamAE">
        <xs:annotation>
            <xs:documentation>
