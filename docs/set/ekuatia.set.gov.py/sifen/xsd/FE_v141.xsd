                        <xs:minLength value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDirVen" type="tdDirec"/>
            <xs:element name="dNumCasVen">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Numero de casa del vendedor
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="6"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
             <xs:element name="cDepVen" type="tDepartamentos"/>
            <xs:element name="dDesDepVen" type="tDesDepartamento"/>
            <xs:element name="cDisVen" minOccurs="0" type="tDistrito"/>
            <xs:element name="dDesDisVen" minOccurs="0" type="tDesDistrito"/>
            <xs:element name="cCiuVen" type="tCiudad"/>
            <xs:element name="dDesCiuVen" type="tDesCiudad"/>
            <xs:element name="dDirProv" type="tdDirec"/>
            <xs:element name="cDepProv" type="tDepartamentos"/>
            <xs:element name="dDesDepProv" type="tDesDepartamento"/>
            <xs:element name="cDisProv" type="tDistrito"/>
            <xs:element name="dDesDisProv" type="tDesDistrito"/>
            <xs:element name="cCiuProv" type="tCiudad"/>
            <xs:element name="dDesCiuProv" type="tDesCiudad"/>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que componen la factura electronica de importacion -->
    <xs:complexType name="tgCamFEI">
        <xs:annotation>
            <xs:documentation>
                Campos que componen la factura electronica de importacion FEI
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="cTipRegImp" type="tcTipRegAdu"/>
            <xs:element name="dPuLleg">
