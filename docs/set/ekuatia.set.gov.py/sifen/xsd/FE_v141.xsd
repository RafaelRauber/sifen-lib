                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="16"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dFecDesp" type="tFecAAAAMMDDguion"/>
            <xs:element name="dNomDesp">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Nombre del despachante
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="60"/>
                        <xs:minLength value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dRucDesp" type="tRuc"/>
            <xs:element name="dDVDesp" type="tDVer"/>
            <xs:element name="cPaisProd" type="paisType"/>
            <xs:element name="dDesPaisProd" type="tDesPais"/>
            <xs:element name="dVend">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Nombre del vendedor
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="30"/>
                        <xs:minLength value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dValorInv">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Valor factura invoice
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="12"/>
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dValorFle">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Valor flete
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="10"/>
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dValorSeg">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Valor seguro
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="10"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minExclusive value="0"/>
			            <xs:maxInclusive value="9999999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dValorImpGs">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Valor imponible en guaranies
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:integer">
			            <xs:minExclusive value="0"/>
			            <xs:maxInclusive value="9999999999"/>
			        </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDerAdu">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Derecho Aduanero
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="12"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minExclusive value="0"/>
			            <xs:maxInclusive value="99999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dIndi">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            INDI
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="12"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minExclusive value="0"/>
			            <xs:maxInclusive value="99999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dSerValor">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Servicio de valoracion
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="12"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minExclusive value="0"/>
			            <xs:maxInclusive value="99999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dIVAImp">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            IVA
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="12"/>
                        <xs:fractionDigits value="4"/>
                        <xs:minExclusive value="0"/>
			            <xs:maxInclusive value="99999999.9999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dTasaIntAd">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            IVA
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:integer">
                    	<xs:minInclusive value="1"/>
                    	<xs:maxInclusive value="99"/>           
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que componen la factura electronica de exportacion-->
    <xs:complexType name="tgCamFEE">
        <xs:annotation>
